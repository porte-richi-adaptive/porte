                                    .interior-complect__view
                                        include ../static/images/content/interior/complect-tab.svg

                                .interior-complect__content(data-complect-content='1')
                                    p.interior-complect__about Декоративные планки. Крепятся вдоль дверного проёма и&nbsp;закрывают щель между полотном и&nbsp;коробкой.
                                    .interior-complect__table
                                        .interior-complect__row.interior-complect__head
                                            .interior-complect__element Параметры
                                            .interior-complect__price мм
                                        .interior-complect__row
                                            .interior-complect__element Капитель
                                            .interior-complect__price 1

                                        .interior-complect__row
                                            .interior-complect__element Опора наличника
                                            .interior-complect__price 4

                                        .interior-complect__row
                                            .interior-complect__element Консоль
                                            .interior-complect__price 4

                                    //img(src='static/images/content/interior/complect-pic.png', alt='').interior-complect__pic

                                .interior-complect__item(data-complect-tab='2')

extends template/main

block variables
    - var title = 'Входная и межкомнатные двери в одном стиле';
    - var headerGrayBackground = true;
    - var headerGrayBackgroundBig = true;

block content
    .interior
        section.interior-first.interior-first--single.interior-first--simple
            .container
                .row
                    .col-16
                        .interior-first__wrap
